{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug",
        "simulator": true
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "NODE_ENV": "development"
      }
    },
    "debug": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug",
        "simulator": false
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "NODE_ENV": "development",
        "EXPO_DEBUG": "true"
      }
    },
    "preview": {
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}